<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:views="clr-namespace:MauiApp1.Components"
             x:Class="Manosque.Maui.Pages.ComlinePage">
    <Grid ColumnDefinitions="*,Auto" RowDefinitions="Auto,*">
        <Entry x:Name="Prompt" Text="Get-Personne"/>
        <Button Grid.Column="1" Clicked="OnComline" Text="Exécuter" HorizontalOptions="FillAndExpand" CornerRadius="20"/>
        <ScrollView Grid.Row="2" Grid.ColumnSpan="2" Orientation="Both" VerticalScrollBarVisibility="Never" HorizontalScrollBarVisibility="Never">
            <StackLayout x:Name="DataTablesStackLayout" Grid.ColumnSpan="2">

            </StackLayout>
        </ScrollView>
    </Grid>
</ContentPage>
